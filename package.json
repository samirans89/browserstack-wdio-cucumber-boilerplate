  "name": "BrowserStack_NodeJS_Cucumber_boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber on BrowserStack",
  "homepage": "https://github.com/samirans89/browserstack-wdio-cucumber-boilerplate",
  "contributors": [
    "Samiran Saha <samiran.s89@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "getUrl": "https://github.com/samirans89/browserstack-wdio-cucumber-boilerplate.git"
  },
  "bugs": {
    "getUrl": "https://github.com/samirans89/browserstack-wdio-cucumber-boilerplate/issues"
  },
  "keywords": [
    "wdio",
    "webdriver",
    "webdriverio",
    "cucumber",
    "test",
    "boilerplate",
    "selenium"
  ],
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "eslint 'src/**/*.js' 'test/**/*.js!(setup.js)'",
